<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Vista laboratorio de masa -->
        <record model="ir.ui.view" id="certificate_wizard_view_form_masa">
          <field name="name">certificate.wizard.view.form.masa</field>
          <field name="model">certificate.wizard</field>
          <field name="arch" type="xml">
            <form string="Asignar Tipo de documento Laboratorio Masa">
              <group>
                <field name="tipo_documento"/>
                <field name="lugar_lab"/>
              </group>
              <footer>
                <button name="update_certificate_number_masa" string="Asignar" type="object" class="btn-primary"/>
                <button string="Cancelar" class="btn-default" special="cancel"/>
              </footer>
            </form>
          </field>
        </record>

